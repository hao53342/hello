<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
  <package name="noticeManage" extends="struts-default" namespace="/admin">
  	<action name="noticeManageAction_*" method="{1}" class="noticeManageAction">
  		<result name="success">noticeManage.jsp</result>
  	</action>
  </package>
  
 <package name="_notice" extends="struts-default" namespace="/">
  	<action name="noticeManageAction_*" method="{1}" class="noticeManageAction">
  		<result name="success">notice.jsp</result>
  	</action>
  </package>
  
</struts>	