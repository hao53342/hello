<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
  <package name="productInfoManage" extends="struts-default" namespace="/admin">
  	<action name="productInfoManageAction_*" method="{1}" class="productInfoManageAction">
  		<result name="success">productInfoManage.jsp</result>
  	</action>
  </package>
  

	<package name="userProductInfo" extends="struts-default" namespace="/user">
  	<action name="productInfoManageAction_*" method="{1}" class="productInfoManageAction">
  		<result name="success">productInfoManage.jsp</result>
  	</action>
  </package>
  
  <package name="product_Info" extends="struts-default" namespace="/">
  	<action name="productInfoManageAction_*" method="{1}" class="productInfoManageAction">
  		<result name="success">product.jsp</result>
  	</action>
  </package>
  
</struts>	